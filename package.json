  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prettier": "prettier 'src/**/*.ts'",
    "prettier:fix": "prettier --write 'src/**/*.ts'",
    "postinstall": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged"
